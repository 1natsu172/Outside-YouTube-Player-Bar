{
	"name": "Outside-YouTube-Player-Bar",
	"private": true,
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/1natsu172/Outside-YouTube-Player-Bar.git"
	},
	"version": "3.0.10",
	"description": "Display YouTube's control bar outside the video.",
	"scripts": {
		"postinstall": "wxt prepare",
		"biome": "biome check --apply ./src",
		"dev": "wxt",
		"dev:arc": "wxt -b arc -c ./wxt.config.arc.ts",
		"dev:chrome": "wxt -b chrome",
		"dev:edge": "wxt -b edge",
		"dev:firefox": "wxt -b firefox",
		"build": "pnpm run --sequential /^build:.*/",
		"build:chrome": "wxt build -b chrome",
		"build:edge": "wxt build -b edge",
		"build:firefox": "wxt build -b firefox",
		"zip": "pnpm run --sequential /^zip:.*/",
		"zip:chrome": "wxt zip -b chrome",
		"zip:edge": "wxt zip -b edge",
		"zip:firefox": "wxt zip -b firefox",
		"test": "pnpm run --sequential /^test:.*/",
		"test:unit": "vitest",
		"test:e2e": "playwright test",
		"test:types": "vitest --typecheck.only --typecheck.tsconfig .tsconfig.json",
		"postversion": "git push origin v$npm_new_version",
		"release-to-git": "node ./scripts/releaseNotesTemplate.mjs | gh release create v$npm_package_version --notes-file - && gh release view v$npm_package_version --web",
		"delivery": "pnpm run --sequential /^delivery:.*/",
		"delivery:chrome": "wxt submit --chrome-zip .output/*-chrome.zip",
		"delivery:edge": "wxt submit --edge-zip .output/*-chrome.zip",
		"delivery:firefox": "wxt submit --firefox-zip .output/*-firefox.zip --firefox-sources-zip .output/*-sources.zip"
	},
	"devDependencies": {
		"@1natsu/wait-element": "^3.0.0",
		"@biomejs/biome": "1.7.3",
		"@playwright/test": "^1.44.0",
		"@testing-library/react": "^15.0.7",
		"@types/node": "^20.12.12",
		"@types/postcss-import": "^14.0.3",
		"@types/react": "^18.3.2",
		"@types/react-dom": "^18.3.0",
		"@types/semver": "^7.5.8",
		"@vitejs/plugin-react": "^4.2.1",
		"chrome-webstore-upload-cli": "^3.1.0",
		"git-semver-tags": "^8.0.0",
		"happy-dom": "^14.11.0",
		"playwright": "^1.44.0",
		"postcss-import": "^16.1.0",
		"postcss-nesting": "^12.1.4",
		"prettier": "^3.2.5",
		"rollup-preserve-directives": "^1.1.1",
		"sass": "^1.75.0",
		"shelljs": "^0.8.5",
		"shx": "^0.3.4",
		"typescript": "^5.4.5",
		"typescript-plugin-css-modules": "^5.1.0",
		"vite-plugin-svgr": "^4.2.0",
		"vitest": "^1.5.0",
		"wxt": "^0.18.2"
	},
	"dependencies": {
		"@mantine/core": "^7.9.1",
		"@mantine/hooks": "^7.9.1",
		"@tabler/icons-react": "^3.4.0",
		"@tanstack/react-form": "^0.19.5",
		"@tanstack/react-query": "^5.36.2",
		"@webext-core/messaging": "^1.4.0",
		"consola": "^3.2.3",
		"defu": "^6.1.4",
		"derive-valtio": "^0.1.0",
		"mabiki": "^1.0.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-error-boundary": "^4.0.13",
		"semver": "^7.6.2",
		"valtio": "^1.13.2",
		"valtio-history": "^0.3.3"
	},
	"packageManager": "pnpm@9.1.2+sha256.19c17528f9ca20bd442e4ca42f00f1b9808a9cb419383cd04ba32ef19322aba7",
	"pnpm": {
		"overrides": {
			"semver@>=7.0.0 <7.5.2": ">=7.5.2",
			"semver@<5.7.2": ">=5.7.2",
			"semver@>=6.0.0 <6.3.1": ">=6.3.1"
		}
	}
}
