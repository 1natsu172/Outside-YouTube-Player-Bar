{
	"name": "Outside-YouTube-Player-Bar",
	"private": true,
	"type": "module",
	"repository": {
		"type": "git",
		"url": "https://github.com/1natsu172/Outside-YouTube-Player-Bar.git"
	},
	"version": "3.0.10",
	"description": "Display YouTube's control bar outside the video.",
	"scripts": {
		"postinstall": "wxt prepare",
		"dev": "wxt",
		"dev:arc": "wxt -b arc -c ./wxt.config.arc.ts",
		"dev:chrome": "wxt -b chrome",
		"dev:edge": "wxt -b edge",
		"dev:firefox": "wxt -b firefox",
		"build": "wxt build",
		"build:chrome": "wxt build -b chrome",
		"build:edge": "wxt build -b edge",
		"build:firefox": "wxt build -b firefox",
		"zip": "wxt zip",
		"zip:chrome": "wxt zip -b chrome",
		"zip:edge": "wxt zip -b edge",
		"zip:firefox": "wxt zip -b firefox",
		"test": "pnpm run --sequential /^test:.*/",
		"test:unit": "vitest",
		"test:e2e": "playwright test",
		"test:tsc": "tsc -p ./tsconfig.json --no-emit",
		"preversion": "node ./scripts/bump-manifest $npm_new_version && git add manifest.json",
		"postversion": "git push origin v$npm_new_version",
		"release-to-git": "node ./scripts/releaseNotesTemplate | gh release create v$npm_package_version --notes-file - && gh release view v$npm_package_version --web",
		"delivery": "pnpm dlx chrome-webstore-upload-cli@3 upload --source dist.zip --auto-publish",
		"biome": "biome check --apply ./src"
	},
	"devDependencies": {
		"@1natsu/wait-element": "^3.0.0",
		"@biomejs/biome": "1.7.0",
		"@playwright/test": "^1.43.1",
		"@testing-library/react": "^15.0.2",
		"@types/node": "^20.12.7",
		"@types/react": "^18.2.79",
		"@types/react-dom": "^18.2.25",
		"@vitejs/plugin-react": "^4.2.1",
		"chrome-webstore-upload-cli": "^3.1.0",
		"fs-extra": "^11.2.0",
		"git-semver-tags": "^7.0.1",
		"happy-dom": "^14.7.1",
		"playwright": "^1.43.1",
		"postcss-nesting": "^12.1.1",
		"prettier": "^3.2.5",
		"sass": "^1.75.0",
		"shelljs": "^0.8.5",
		"shx": "^0.3.4",
		"typescript": "^5.4.5",
		"typescript-plugin-css-modules": "^5.1.0",
		"vite-plugin-svgr": "^4.2.0",
		"vitest": "^1.5.0",
		"wxt": "^0.17.0"
	},
	"dependencies": {
		"consola": "^3.2.3",
		"derive-valtio": "^0.1.0",
		"mabiki": "^1.0.1",
		"react": "18.3.0-canary-c3048aab4-20240326",
		"react-dom": "18.3.0-canary-c3048aab4-20240326",
		"valtio": "^1.13.2"
	},
	"packageManager": "pnpm@9.0.4+sha256.caa915eaae9d9aefccf50ee8aeda25a2f8684d8f9d5c6e367eaf176d97c1f89e",
	"pnpm": {
		"overrides": {
			"semver@>=7.0.0 <7.5.2": ">=7.5.2",
			"semver@<5.7.2": ">=5.7.2",
			"semver@>=6.0.0 <6.3.1": ">=6.3.1",
			"minimatch@<3.0.5": ">=3.0.5"
		}
	}
}
